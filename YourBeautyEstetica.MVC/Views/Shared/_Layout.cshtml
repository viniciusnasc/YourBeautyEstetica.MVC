<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - YourBeautyEstetica.MVC</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    @*<link rel="stylesheet" href="~/YourBeautyEstetica.MVC.styles.css" asp-append-version="true" />*@

    <link rel="stylesheet" href="~/Tempcss/reset.css" type="text/css" media="screen">
	<link rel="stylesheet" href="~/Tempcss/skeleton.css" type="text/css" media="screen">
	<link rel="stylesheet" href="~/Tempcss/superfish.css" type="text/css" media="screen">
	<link rel="stylesheet" href="~/Tempcss/style.css" type="text/css" media="screen">
	<link rel="stylesheet" href="~/Tempcss/slider.css" type="text/css" media="screen">
	<link rel="stylesheet" href="~/Tempcss/WiltoCarousel.css" type="text/css" media="screen">
	<link rel="stylesheet" href="~/Tempcss/gallery.css" type="text/css" media="screen">
	<link rel="stylesheet" href="~/Tempcss/ie.css" type="text/css" media="screen">
</head>
<body id="page2">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">YourBeautyEstetica.MVC</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" supress-by-role="Funcionario" asp-area="" asp-controller="Home" asp-action="Administracao">Administracao</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

  
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

  <script src="~/Tempjs/gallery.js"></script>
  <script src="~/Tempjs/jquery-1.7.1.min.js"></script>
	<script src="~/Tempjs/script.js"></script>
	<script src="~/Tempjs/jquery.hoverIntent.js"></script>
	<script src="~/Tempjs/superfish.js"></script>
	<script src="~/Tempjs/jquery.responsivemenu.js"></script>
	<script src="~/Tempjs/jquery.tmpl.min.js" type="text/javascript"></script>
	<script src="~/Tempjs/jquery.easing.1.3.js" type="text/javascript"></script>
	<script type="text/javascript" src="~/Tempjs/jquery.elastislide.js"></script>
    <script id="img-wrapper-tmpl" type="text/x-jquery-tmpl">
		{{if itemsCount > 1}}
			<div class="rg-image-nav">
				<a href="#" class="rg-image-nav-prev">Previous</a>
				<a href="#" class="rg-image-nav-next">Next</a>
			</div>
		{{/if}}
		<div class="rg-image"></div>
		<div class="rg-loading"></div>
		<div class="rg-caption-wrapper">
			<div class="rg-caption" style="display:none;"><p></p></div>
		</div>
	</script>
	<script>
		$(function(){$('.es-carousel li a img')
			.mouseover(function(){$(this).stop().animate({opacity:0.6}, 300)})
			.mouseout(function(){$(this).stop().animate({opacity:1}, 300)});
		});
	</script>

    @await RenderSectionAsync("Scripts", required: false)

    <footer class="border-top footer text-muted">
        <div class="container_12">
			<div class="wrapper">
				<div class="grid_12 footer-text">
					<p class="p0">Your Beauty Estética © 2022&nbsp;&nbsp;|&nbsp;&nbsp;<a href="index-6.html">Privacy Policy</a></p>
				</div>
			</div>
		</div>
        @*<div class="container">
            &copy; 2022 - YourBeautyEstetica.MVC - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>*@
    </footer>
</body>
  
</html>